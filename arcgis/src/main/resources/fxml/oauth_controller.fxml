<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.wu.arcgis.gui.OAuthController">
	<children>
		<GridPane layoutX="29.0" layoutY="25.0" prefHeight="90.0" prefWidth="263.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Label text="用户名：" />
				<!--<TextField fx:id="userName" GridPane.rowIndex="0" GridPane.columnIndex="1"/>-->
				<Label text="密 码：" GridPane.rowIndex="1" />
				<!--<TextField fx:id="password" GridPane.rowIndex="1" GridPane.columnIndex="1"/>-->
				<Button fx:id="loginButton" mnemonicParsing="false" onAction="#authenticate" text="登 陆：" GridPane.rowIndex="2">
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Button>
				<Button fx:id="resetButton" defaultButton="true" mnemonicParsing="false" cancelButton="true" text="取消：" GridPane.columnIndex="1" GridPane.rowIndex="2">
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Button>
				<TextField fx:id="userName" GridPane.columnIndex="1" />
				<TextField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
			</children>
		</GridPane>
	</children>
</AnchorPane>

